package com.jsp.employee_servlet_project.dto;

public class Employee {
	private String name;
	private String mail;
	private Long phone;
	private String pwd;
	private int age;
	private String desig;
	private double sal;
	private String address;

	public Employee(String name, String mail, Long phone, String pwd, int age, String desig, Double sal,
			String address) {
		super();
		this.name = name;
		this.mail = mail;
		this.phone = phone;
		this.pwd = pwd;
		this.age = age;
		this.desig = desig;
		this.sal = sal;
		this.address = address;
	}
}
