package studentSpring;

public class Batch {
	private int rNo;
	private String stream;
	@Override
	public String toString() {
		return "Batch [rNo=" + rNo + ", Stream=" + stream + "]";
	}
	public int getrNo() {
		return rNo;
	}
	public void setrNo(int rNo) {
		this.rNo = rNo;
	}
	public String getStream() {
		return stream;
	}
	public void setStream(String stream) {
		stream = stream;
	}
}
